<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mapFragment">

    <action
        android:id="@+id/action_mapFragment_to_mapFragment"
        app:destination="@id/mapFragment" />
    <argument
        android:name="Settings"
        app:argType="com.example.position2shp.Settings.Settings"
        app:nullable="true"
        android:defaultValue="@null"/>

    <fragment
        android:id="@+id/mapFragment"
        android:name="com.example.position2shp.Map.MapFragment"
        android:label="MapFragment"
        tools:layout="@layout/mapview">
        <action
            android:id="@+id/action_mapFragment_to_mapEditFragment"
            app:destination="@id/mapEditFragment" />
        <action
            android:id="@+id/action_mapFragment_to_settingsContainerFragment"
            app:destination="@id/settingsContainerFragment" />
        <argument
            android:name="Settings"
            app:argType="com.example.position2shp.Settings.Settings"
            app:nullable="true"
            android:defaultValue="@null" />

    </fragment>
    <fragment
        android:id="@+id/mapEditFragment"
        android:name="com.example.position2shp.Map.MapEditFragment"
        android:label="MapEditFragment"
        tools:layout="@layout/mapeditview">
        <action
            android:id="@+id/action_mapEditFragment_to_mapFragment"
            app:destination="@id/mapFragment" />
        <argument
            android:name="Settings"
            app:argType="com.example.position2shp.Settings.Settings"
            app:nullable="true"
            android:defaultValue="@null" />
    </fragment>
    <fragment
        android:id="@+id/settingsContainerFragment"
        android:name="com.example.position2shp.Settings.SettingsContainerFragment"
        android:label="SettingsContainerFragment" >
        <action
            android:id="@+id/action_settingsContainerFragment_to_mapFragment"
            app:destination="@id/mapFragment" />
        <argument
            android:name="Settings"
            app:argType="com.example.position2shp.Settings.Settings"
            app:nullable="true"
            android:defaultValue="@null"/>
    </fragment>
</navigation>